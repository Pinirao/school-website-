{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "build",
  "installCommand": "npm install",
  "functions": {
    "src/api/**": {
      "maxDuration": 30
    }
  },
  "build": {
    "env": {
      "CI": "false"
    },
    "commands": {
      "install": "npm install",
      "build": "npm run build"
    }
  }
}